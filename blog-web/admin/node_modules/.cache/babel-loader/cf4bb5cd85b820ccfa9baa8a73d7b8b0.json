AAA,KAAA,CAAA,GAAA,CAAA,KAAA,EAAA,IAAA,CAAA,iBAAA;AAAA,UAAA,IAAA,SAAA,IAAA;AACA,MAAA,KAAA,CAAA,OAAA,GAAA,IAAA,CAAA,IAAA;AACA,KAFA;AAGA,GAnBA;AAqBA,EAAA,IAAA,EAAA,gBAAA;AACA,WAAA;AACA,MAAA,OAAA,EAAA,EADA;AAEA,MAAA,KAAA,EAAA,EAFA;AAGA,MAAA,SAAA,EAAA,KAHA;AAIA,MAAA,OAAA,EAAA;AACA,QAAA,EAAA,EAAA,IADA;AAEA,QAAA,KAAA,EAAA,IAFA;AAGA,QAAA,OAAA,EAAA,EAHA;AAIA,QAAA,IAAA,EAAA,EAJA;AAKA,QAAA,IAAA,EAAA,EALA;AAMA,QAAA,KAAA,EAAA,EANA;AAOA,QAAA,MAAA,EAAA,CAPA;AAQA,QAAA,MAAA,EAAA;AARA;AAJA,KAAA;AAeA,GArCA;AAsCA,EAAA,OAAA,EAAA;AACA,IAAA,MADA,kBACA,KADA,EACA,MADA,EACA;AACA,WAAA,OAAA,CAAA,OAAA,GAAA,KAAA;AACA,WAAA,OAAA,CAAA,IAAA,GAAA,MAAA,CAFA,CAEA;AACA,KAJA;AAKA,IAAA,WALA,uBAKA,QALA,EAKA;AACA,WAAA,OAAA,CAAA,MAAA,GAAA,QAAA,CAAA,IAAA,CAAA,GAAA;AACA,KAPA;AAQA,IAAA,mBARA,iCAQA;AAAA;;AACA,UAAA,IAAA,GAAA,KAAA,MAAA,CAAA,IAAA;AACA,UAAA,GAAA,GAAA,IAAA,CAAA,KAAA,CAAA,GAAA,CAAA;AACA,UAAA,SAAA,GAAA,GAAA,CAAA,CAAA,CAAA;AACA,UAAA,GAAA,GAAA,MAAA;;AACA,UAAA,KAAA,OAAA,CAAA,KAAA,CAAA,IAAA,MAAA,EAAA,EAAA;AACA,aAAA,QAAA,CAAA,KAAA,CAAA,UAAA;AACA,eAAA,KAAA;AACA;;AACA,UAAA,KAAA,OAAA,CAAA,OAAA,CAAA,IAAA,MAAA,EAAA,EAAA;AACA,aAAA,QAAA,CAAA,KAAA,CAAA,UAAA;AACA,eAAA,KAAA;AACA;;AACA,UAAA,KAAA,OAAA,CAAA,GAAA,IAAA,EAAA,EAAA;AACA,aAAA,QAAA,CAAA,KAAA,CAAA,UAAA;AACA,eAAA,KAAA;AACA;;AACA,UAAA,CAAA,SAAA,EAAA;AACA,aAAA,OAAA,CAAA,IAAA,GAAA,KAAA,OAAA,CAAA,IAAA,IAAA,EAAA,EAAA,MAAA,CAAA,YAAA,CAAA;AACA;;AAEA,WAAA,KAAA,CAAA,IAAA,CAAA,GAAA,EAAA,KAAA,OAAA,EAAA,IAAA,CAAA,iBAAA;AAAA,YAAA,IAAA,SAAA,IAAA;;AACA,YAAA,IAAA,CAAA,IAAA,IAAA,CAAA,EAAA;AACA,UAAA,MAAA,CAAA,OAAA,CAAA,OAAA,CAAA;AACA,YAAA,KAAA,EAAA,IADA;AAEA,YAAA,OAAA,EAAA,IAAA,CAAA;AAFA,WAAA;AAIA,SALA,MAKA;AACA,UAAA,MAAA,CAAA,OAAA,CAAA,KAAA,CAAA;AACA,YAAA,KAAA,EAAA,IADA;AAEA,YAAA,OAAA,EAAA,IAAA,CAAA;AAFA,WAAA;AAIA;;AACA,QAAA,MAAA,CAAA,SAAA,GAAA,KAAA;AACA,OAbA;AAcA;AA3CA;AAtCA,CAAA","sourcesContent":["<template>\n  <el-card class=\"main-card\">\n    <!-- 文章标题 -->\n    <div class=\"article-title-container\">\n      <el-input\n        v-model=\"article.title\"\n        size=\"medium\"\n        placeholder=\"输入文章标题\"\n      />\n      <el-button\n        type=\"danger\"\n        size=\"medium\"\n        @click=\"addOrEdit = true\"\n        style=\"margin-left:10px\"\n      >\n        发布文章\n      </el-button>\n    </div>\n    <!-- 文章内容 -->\n    <mavon-editor\n      ref=\"md\"\n      v-model=\"value\"\n      style=\"height:calc(100vh - 225px)\"\n      @change=\"change\"\n    />\n    <!-- 添加文章对话框 -->\n    <el-dialog :visible.sync=\"addOrEdit\" width=\"40%\" top=\"10vh\">\n      <div class=\"dialog-title-container\" slot=\"title\">\n        上传文章\n      </div>\n      <!-- 文章数据 -->\n      <el-form label-width=\"80px\" size=\"medium\" :model=\"article\">\n        <el-form-item label=\"文章标签\">\n          <el-select\n            v-model=\"article.tagId\"\n            placeholder=\"请选择标签\"\n          >\n            <el-option\n              v-for=\"item in tagList\"\n              :key=\"item.id\"\n              :label=\"item.tagName\"\n              :value=\"item.id\"\n            />\n          </el-select>\n        </el-form-item>\n        <el-form-item label=\"上传封面\">\n          <el-upload\n            class=\"upload-cover\"\n            drag\n            action=\"http://autwind.cn:5000/v1/upload\"\n            multiple\n            :on-success=\"uploadCover\"\n          >\n            <i class=\"el-icon-upload\" v-if=\"article.images == ''\" />\n            <div class=\"el-upload__text\" v-if=\"article.images == ''\">\n              将文件拖到此处，或<em>点击上传</em>\n            </div>\n            <img\n              v-else\n              :src=\"article.images\"\n              width=\"360px\"\n              height=\"180px\"\n            />\n          </el-upload>\n        </el-form-item>\n      </el-form>\n      <div slot=\"footer\">\n        <el-button @click=\"addOrEdit = false\">取 消</el-button>\n        <el-button type=\"danger\" @click=\"saveOrUpdateArticle\">\n          发 表\n        </el-button>\n      </div>\n    </el-dialog>\n  </el-card>\n</template>\n\n<script>\nexport default {\n  created() {\n    const path = this.$route.path;\n    const arr = path.split(\"/\");\n    const articleId = arr[2];\n    if (articleId) {\n      this.$http.get(\"blog/\" + articleId).then(({ data }) => {\n        this.article = data.data;\n        this.value = data.data.content;\n      });\n    }else{\n      this.$http.get(\"blog/latest\").then(({ data }) => {\n        const blogId = data.data.blogId\n        this.article.blogId = blogId + 100;\n      });\n    }\n    this.$http.get(\"tag\").then(({ data }) => {\n      this.tagList = data.data;\n    });\n  },\n\n  data: function() {\n    return {\n      tagList:[],\n      value: \"\",\n      addOrEdit: false,\n      article: {\n        id: null,\n        tagId: null,\n        content: \"\",\n        html: \"\",\n        time: \"\",\n        title: \"\",\n        blogId: 0,\n        images: \"\"\n      }\n    };\n  },\n  methods: {\n    change(value, render) {\n      this.article.content = value;\n      this.article.html = render; // 解析的html\n    },\n    uploadCover(response) {\n      this.article.images = response.data.url;\n    },\n    saveOrUpdateArticle() {\n      const path = this.$route.path;\n      const arr = path.split(\"/\");\n      const articleId = arr[2];\n      var url = \"blog\";\n      if (this.article.title.trim() == \"\") {\n        this.$message.error(\"文章标题不能为空\");\n        return false;\n      }\n      if (this.article.content.trim() == \"\") {\n        this.$message.error(\"文章内容不能为空\");\n        return false;\n      }\n      if (this.article.tag == \"\") {\n        this.$message.error(\"文章标签不能为空\");\n        return false;\n      }\n      if (!articleId) {\n        this.article.time = this.$moment(new Date()).format(\"YYYY-MM-DD\");\n      }\n\n      this.$http.post(url, this.article).then(({ data }) => {\n        if (data.code == 1) {\n          this.$notify.success({\n            title: \"成功\",\n            message: data.message\n          });\n        } else {\n          this.$notify.error({\n            title: \"失败\",\n            message: data.message\n          });\n        }\n        this.addOrEdit = false;\n      });\n    }\n  }\n};\n</script>\n\n<style scoped>\n.article-title-container {\n  display: flex;\n  align-items: center;\n  margin-bottom: 1.25rem;\n}\n.save-btn {\n  margin-left: 0.75rem;\n  background: #fff;\n  color: #f56c6c;\n}\n</style>\n"],"sourceRoot":"src/views/article"}]}